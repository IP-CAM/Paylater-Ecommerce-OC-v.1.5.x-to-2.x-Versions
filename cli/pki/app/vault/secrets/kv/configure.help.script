#!/bin/bash
function vault-secrets-kv-configure-help {
    echo -e ""
    echo -e "Configure K/V secrets for application to specific environment"
    echo -e "${YELLOW}Format:${NC} ${GREEN}./pkictl${NC} vault secrets kv configure {APPLICATION_NAME} {ENVIRONMENT}"
    echo -e "${YELLOW}Example:${NC} ${GREEN}./pkictl${NC} vault secrets kv configure sampleapp dev"
}

vault-secrets-kv-configure-help