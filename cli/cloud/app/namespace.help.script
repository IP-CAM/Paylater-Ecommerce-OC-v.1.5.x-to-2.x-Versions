#!/bin/bash
function namespace-help {
    echo -e "Namespace should be set properly as a file ${YELLOW}NAMESPACE${NC} in ${YELLOW}root CLI${NC} folder."
    if [ ! -f "./NAMESPACE" ]; then
        echo -e "- ${YELLOW}./NAMESPACE${NC} ${RED}[FAILED]${NC}" 
    else
        echo -e "- ${YELLOW}./NAMESPACE${NC} ${GREEN}[SUCCESS]${NC}"    
    fi
}

namespace-help